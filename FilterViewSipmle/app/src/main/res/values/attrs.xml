<?xml version="1.0" encoding="utf-8"?>
<resources>
    
    <declare-styleable name="LabelSeekBar">
        
        <!-- TextView attributes -->
        <attr name="android:textColor"/>
        <attr name="android:textSize"/>
        
        <!-- SeekBar attributes -->
        <attr name="android:thumb"/>
        <attr name="android:progress"/>
        <attr name="android:progressDrawable"/>
        
        <!-- LabelSeekBar attributes -->
        <attr name="min" format="float"/>
        <attr name="max" format="float"/>
        <attr name="step" format="float"/>
        <attr name="seekBarLeftMargin" format="dimension"/>
        <attr name="seekBarRightMargin" format="dimension"/>
        <attr name="seekBarWidth" format="dimension"/>
        <attr name="seekBarHeight" format="dimension"/>
        <attr name="labelText" format="string"/>
        <attr name="labelTextColor" format="color"/>
        <attr name="labelTextSize" format="dimension"/>
        <attr name="valueUnitText" format="string"/>
        <attr name="valueTextSize" format="dimension"/>
        <attr name="valueTextColor" format="color"/>
        <attr name="valueTextFocusColor" format="color"/>
        <attr name="valueBackground" format="color" />
        <attr name="valueFocusBackground" format="color"/>
        <attr name="editable" format="boolean"/>
    </declare-styleable>

    <attr name="cropImageStyle" format="reference" />





    <declare-styleable name="DragTopLayout">

        <!--Set the collapse panel offset-->
        <attr name="dtlCollapseOffset" format="dimension"/>

        <!--Set enable drag over.The default value is true.-->
        <attr name="dtlOverDrag" format="boolean"/>

        <!--Open the top view after the drag layout is created. The default value is false.-->
        <attr name="dtlOpen" format="boolean"/>

        <!-- The id of the top view (which will be dragged) -->
        <attr name="dtlTopView" format="reference" />

        <!-- The content view which will be dragged together with "dtlTopView" -->
        <attr name="dtlDragContentView" format="reference" />

        <!-- True the top view can be dragged -->
        <attr name="dtlCaptureTop" format="boolean" />
    </declare-styleable>

    <declare-styleable name="NumberPickerStyle">
        <attr name="number_picker_divider" format="reference|color" />
    </declare-styleable>


    <!--add dushiguang E-->


    <!--add dushiguang E-->





    <declare-styleable name="RatingBar">
        <!--尺寸值-->
        <attr name="starImageSize" format="dimension" />
        <!--星星间距-->
        <attr name="starPadding" format="dimension" />
        <!--星星总数-->
        <attr name="starCount" format="integer" />
        <!--空白的星星资源文件值-->
        <attr name="starEmpty" format="reference" />
        <!--满星资源文件值-->
        <attr name="starFill" format="reference" />
        <!--半星资源文件值-->
        <attr name="starHalf" format="reference" />
        <!--是否可点击boolean值-->
        <attr name="clickable" format="boolean" />
        <!--当前进度float值-->
        <attr name="starStep" format="float" />
        <!--每次进度方式的值，整星还是半星-->
        <attr name="stepSize">
            <enum name="Half" value="0" />
            <enum name="Full" value="1" />
        </attr>
    </declare-styleable>






    <!--自定义圆角图片-->
    <declare-styleable name="RoundRectImageView">
        <attr name="corner_radius" format="dimension" />
    </declare-styleable>




</resources>
